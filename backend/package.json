{
  "name": "fitness-site",
  "version": "1.0.0",
  "description": "This is a fitness content web application",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node --openssl-legacy-provider index.js",
    "dev": "cross-env NODE_ENV=development nodemon --openssl-legacy-provider index.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --forceExit",
    "start:test": "NODE_ENV=test node index.js",
    "build:ui": "rm -rf build && cd ../frontend/ && npm run build && cp -r build ../backend",
    "deploy": "fly deploy",
    "deploy:full": "npm run build:ui && npm run deploy",
    "logs:prod": "fly logs"
  },
  "author": "AlexAllen247",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "google-auth-library": "^9.0.0",
    "googleapis": "^123.0.0",
    "helmet": "^7.0.0",
    "jest": "^29.6.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.3.2",
    "nodemailer": "^6.9.3",
    "path": "^0.12.7",
    "pdfkit": "^0.14.0",
    "uuidv4": "^6.2.13"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "supertest": "^6.3.3"
  }
}
